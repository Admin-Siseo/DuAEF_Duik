<Effect matchname="DUIK_One_Layer_IK" name="$$$/AE/Preset/OneLayerIK=1ik">
    <Slider name="$$$/AE/Preset/Weight=Weight" default="100" valid_min="-1000" valid_max="1000" slider_min="0" slider_max="100" precision="1" DISPLAY_PERCENT="true"/>
    <Checkbox name="$$$/AE/Preset/Reverse=Reverse" default="true"/>
    <Angle name="$$$/AE/Preset/FK=FK" default="0"/>
</Effect>



<Effect matchname="DUIK_Two_Layer_IK" name="$$$/AE/Preset/2ik=2ik">
    <Checkbox name="$$$/AE/Preset/Clockwise=Clockwise" default="false"/>
    <Group name="$$$/AE/Preset/FK=FK">
        <Checkbox name="$$$/AE/Preset/DisableIK=Disable IK" default="false"/>
        <Angle name="$$$/AE/Preset/UpperFK=Upper FK" default="0"/>
        <Angle name="$$$/AE/Preset/LowerFK=Lower FK" default="0"/>
    </Group>
    <Group name="$$$/AE/Preset/Stretch=Stretch">
        <Checkbox name="$$$/AE/Preset/Auto-Stretch=Auto-Stretch" default="true"/>
        <Slider name="$$$/AE/Preset/Stretch=Stretch" default="0" valid_min="-1000" valid_max="1000" slider_min="-100" slider_max="100" precision="1"/>
    </Group>
</Effect>