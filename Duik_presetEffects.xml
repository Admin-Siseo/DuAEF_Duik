	<!-- BEGIN DUIK PSEUDO EFFECTS -->
  
  <Effect matchname="DUIK_One_Layer_IK" name="$$$/AE/Preset/1ik=1ik">
    <Slider name="$$$/AE/Preset/Weight=Weight" default="100" valid_min="-1000" valid_max="1000" slider_min="0" slider_max="100" precision="1" DISPLAY_PERCENT="true"/>
    <Checkbox name="$$$/AE/Preset/Reverse=Reverse" default="true"/>
    <Angle name="$$$/AE/Preset/FK=FK" default="0"/>
  </Effect>

  <Effect matchname="DUIK_Two_Layer_IK" name="$$$/AE/Preset/2ik=2ik">
    <Checkbox name="$$$/AE/Preset/Clockwise=Clockwise" default="false"/>
    <Group name="$$$/AE/Preset/FK=FK">
        <Checkbox name="$$$/AE/Preset/DisableIK=Disable IK" default="false"/>
        <Angle name="$$$/AE/Preset/UpperFK=Upper FK" default="0"/>
        <Angle name="$$$/AE/Preset/LowerFK=Lower FK" default="0"/>
    </Group>
    <Group name="$$$/AE/Preset/Stretch=Stretch">
        <Checkbox name="$$$/AE/Preset/Auto-Stretch=Auto-Stretch" default="true"/>
        <Slider name="$$$/AE/Preset/Stretch=Stretch" default="0" valid_min="-1000" valid_max="1000" slider_min="-100" slider_max="100" precision="1"/>
    </Group>
  </Effect>
  
  	<Effect matchname="DUIK_3D_Wiggle" name="$$$/AE/Preset/3dwiggle=3dwiggle">
		<Group name="$$$/AE/Preset/Amplitudes=Amplitudes">
			<Slider name="$$$/AE/Preset/XAmp=X Amp." default="0" valid_min="0" valid_max="10000" slider_min="0" slider_max="1000" precision="1"/>
			<Slider name="$$$/AE/Preset/YAmp=Y Amp." default="0" valid_min="0" valid_max="10000" slider_min="0" slider_max="1000" precision="1"/>
			<Slider name="$$$/AE/Preset/ZAmp=Z Amp." default="0" valid_min="0" valid_max="10000" slider_min="0" slider_max="1000" precision="1"/>
		</Group>
		<Group name="$$$/AE/Preset/Frequencies=Frequencies">
			<Slider name="$$$/AE/Preset/XFrequ=X Freq." default="0" valid_min="0" valid_max="1000" slider_min="0" slider_max="100" precision="1"/>
			<Slider name="$$$/AE/Preset/YFreq=Y Freq." default="0" valid_min="0" valid_max="1000" slider_min="0" slider_max="100" precision="1"/>
			<Slider name="$$$/AE/Preset/ZFreq=Z Freq" default="0" valid_min="0" valid_max="1000" slider_min="0" slider_max="100" precision="1"/>
		</Group>
	</Effect>
	
	<Effect matchname="DUIK_2D_Wiggle" name="$$$/AE/Preset/2dwiggle=2dwiggle">
		<Group name="$$$/AE/Preset/Amplitudes=Amplitudes">
			<Slider name="$$$/AE/Preset/XAmp=X Amp." default="0" valid_min="0" valid_max="10000" slider_min="-1000" slider_max="1000" precision="1"/>
			<Slider name="$$$/AE/Preset/YAmp=Y Amp." default="0" valid_min="0" valid_max="10000" slider_min="-1000" slider_max="1000" precision="1"/>
		</Group>
		<Group name="$$$/AE/Preset/Frequencies=Frequencies">
			<Slider name="$$$/AE/Preset/XFreq=X Freq." default="0" valid_min="0" valid_max="1000" slider_min="0" slider_max="100" precision="1"/>
			<Slider name="$$$/AE/Preset/YFreq=Y Freq." default="0" valid_min="0" valid_max="1000" slider_min="0" slider_max="100" precision="1"/>
		</Group>
	</Effect>
	
	<Effect matchname="DUIK_1D_Wiggle" name="$$$/AE/Preset/1dwiggle=1dwiggle">
		<Slider name="$$$/AE/Preset/Amplitude=Amplitude" default="0" valid_min="0" valid_max="10000" slider_min="0" slider_max="1000" precision="1"/>
		<Slider name="$$$/AE/Preset/Frequency=Frequency" default="0" valid_min="0" valid_max="1000" slider_min="0" slider_max="100" precision="1"/>
	</Effect>
	
	<Effect matchname="DUIK_Exposure" name="$$$/AE/Preset/exposure=exposure">
		<Slider name="$$$/AE/Preset/Exposure=Exposure (frames)" default="1" valid_min="1" valid_max="1000" slider_min="1" slider_max="30" precision="1"/>
	</Effect>
	
		<Effect matchname="DUIK_RotMorph" name="$$$/AE/Preset/rotmorph=rotmorph">
		    <Layer name="$$$/AE/Preset/ReferenceLayer=Reference Layer" default_self="true"/>
			<Angle name="$$$/AE/Preset/Min.Angle=Min. Angle" default="0"/>
			<Angle name="$$$/AE/Preset/Max.Angle=Max. Angle" default="0"/>
	</Effect>
	
		<Effect matchname="DUIK_Swing" name="$$$/AE/Preset/swing=swing">
    <Slider name="$$$/AE/Preset/Amplitude=Amplitude" default="0" valid_min="0" valid_max="10000" slider_min="0" slider_max="100" precision="1"/>
    <Slider name="$$$/AE/Preset/Frequency=Frequency" default="0" valid_min="0" valid_max="100" slider_min="0" slider_max="50" precision="1"/>
    <Slider name="$$$/AE/Preset/Offset=Offset" default="0" valid_min="-100" valid_max="100" slider_min="-10" slider_max="10" precision="1"/>
    <Slider name="$$$/AE/Preset/Damping=Damping" default="0" valid_min="0" valid_max="100" slider_min="0" slider_max="100" precision="1"/>
</Effect>

<Effect matchname="DUIK_Wheel" name="$$$/AE/Preset/wheel=wheel">
		<Slider name="$$$/AE/Preset/Radius=Radius" default="100" valid_min="1" valid_max="10000" slider_min="1" slider_max="1000" precision="1"/>
		<Checkbox name="$$$/AE/Preset/Reverse=Reverse" default="false"/>
	</Effect>
	
	<Effect matchname="DUIK_LensFlare" name="$$$/AE/Preset/lensflare=lensflare">
		<Slider name="$$$/AE/Preset/Intensity=Intensity" default="100" valid_min="0" valid_max="1000" slider_min="0" slider_max="200" precision="1" DISPLAY_PERCENT="true"/>
		<Slider name="$$$/AE/Preset/Scale=Scale" default="100" valid_min="0" valid_max="1000" slider_min="0" slider_max="200" precision="1" DISPLAY_PERCENT="true"/>
		</Effect>
		
		<Effect matchname="DUIK_LensFlareDistance" name="$$$/AE/Preset/lensflaredistance=lensflaredistance">
		<Slider name="$$$/AE/Preset/Distance=Distance" default="0" valid_min="-1000" valid_max="1000" slider_min="-100" slider_max="100" precision="1" DISPLAY_PERCENT="true"/>
		</Effect>

		
		<Effect matchname="DUIK_DistanceLink" name="$$$/AE/Preset/distancelink=distancelink">
    <Group name="$$$/AE/Preset/Range=Range">
        <Slider name="$$$/AE/Preset/MinimumDistance=Minimum Distance" default="0" valid_min="0" valid_max="10000" slider_min="0" slider_max="1000" precision="1"/>
        <Slider name="$$$/AE/Preset/MaximumDistance=Maximum Distance" default="500" valid_min="0" valid_max="10000" slider_min="0" slider_max="1000" precision="1"/>
    </Group>
    <Checkbox name="$$$/AE/Preset/Reverse=Reverse" default="false"/>
    <Slider name="$$$/AE/Preset/Falloff=Falloff" default="10" valid_min="0" valid_max="1000" slider_min="0" slider_max="100" precision="1"/>
</Effect>
    <!-- END DUIK PSEUDO EFFECTS -->
